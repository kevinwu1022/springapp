<!-- for listing the log files. Used in adminServlet -->
<tag>
    <name>logList</name>
    <tagclass>com.hp.taglib.LogListTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>Lists log files</info>
    <attribute>
