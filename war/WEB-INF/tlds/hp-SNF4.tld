<tag>
    <name>link</name>
    <tagclass>com.hp.hpweblib.LinkTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>
        Generates a new hp web link
    </info>
    <attribute>
        <name>name</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
