<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
<tlibversion>1.0</tlibversion>
<jspversion>1.1</jspversion>
<shortname>sso</shortname>
<uri>/hp-SNF4</uri>
<info>Custom Tags for SSO related HTML functionality</info>

<!-- these are new tags that are all in the com.hp.hpweblib package. They 
 support both the old non-Struts sso application as well as Struts/Tiles 
 applications such as Software Analysis Tool (Quincy) -->
<tag>
    <name>leftNav</name>
    <tagclass>com.hp.hpweblib.LeftNavTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        Generates the new HP design left nav table. If you are using the 
        pageContent tag in your pages, do not use this tag, but use 
        'pageLeftNav' instead. 
    </info>
    <attribute>
        <name>title</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleKey</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>bundle</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleURL</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>showDocCart</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>printVersion</name>
    <tagclass>com.hp.hpweblib.PrintVersionTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        includes content if printable version
    </info>
</tag>
<tag>
    <name>notPrintVersion</name>
    <tagclass>com.hp.hpweblib.NotPrintVersionTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        includes content if NOT printable version
    </info>
</tag>
<tag>
    <name>isOvNav</name>
    <tagclass>com.hp.hpweblib.OvNavTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        includes content if openview navigation should be used
    </info>
</tag>
<tag>
    <name>isNotOvNav</name>
    <tagclass>com.hp.hpweblib.NotOvNavTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        includes content if openview navigation should NOT be used
        (HP management software should be used)
    </info>
</tag>
<tag>
    <name>contentBlock</name>
    <tagclass>com.hp.hpweblib.ContentBlockTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        Generates new hp web ContentBlock with a given set of attributes
    </info>
    <attribute>
        <name>width</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>theme</name>
        <!--If not specified, uses the page's theme -->
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>background</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>style</name>
        <!--specifies the style of the content block.
        Allowed values: 'A', 'Acolored', 'B', 'C', 'Ccolored'. -->
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>headerKey</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>header</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>bundle</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>headerURL</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>hr</name>
    <tagclass>com.hp.hpweblib.HrTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>
        Generates a horizontal rule with given width (or default 560)
    </info>
    <attribute>
        <name>width</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>color</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>alternatingColor</name>
    <tagclass>com.hp.hpweblib.AlternatingColorTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>
        Returns two color codes in alternating fashion
    </info>
    <attribute>
        <name>color1</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>color2</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>reset</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>spacer</name>
    <tagclass>com.hp.hpweblib.SpacerTag</tagclass>
