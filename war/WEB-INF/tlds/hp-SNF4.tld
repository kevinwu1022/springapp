<!-- the following tags are in the com.hp.hpweblib.standalone package.
     You shouldn't need these if you are using a Struts/Tiles framework, 
     but if you're not, these will come in handy. 
 -->
<tag>
    <name>pageContent</name>
    <tagclass>com.hp.hpweblib.standalone.PageContentTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        encapsulates pretty much the entire HP new design page 
        (except for headers and footer)
    </info>
    <attribute>
        <name>title</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleKey</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>tagline</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>taglineKey</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>bundle</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>printURL</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>leftNav</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>pageTitle</name>
    <tagclass>com.hp.hpweblib.standalone.PageTitleTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>
        used inside PageContentTag to override title value
    </info>
    <attribute>
        <name>title</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleKey</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>pageLeftNav</name>
    <tagclass>com.hp.hpweblib.standalone.LeftNavTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        Generates the new HP design left nav table. Use this tag inside 
        the pageContent tag. If not using the pageContent tag, use 
        'leftNav' instead. 
    </info>
    <attribute>
        <name>title</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleKey</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>bundle</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleURL</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <name>titleURLKey</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>showDocCart</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
</tag>
<tag>
    <name>leftColumnContent</name>
    <tagclass>com.hp.hpweblib.standalone.LeftColumnContentTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
        Adds content to the left column (use inside the pageLeftNav tag)
    </info>
</tag>
<tag>
    <name>breadcrumbLink</name>
    <tagclass>com.hp.hpweblib.standalone.BreadcrumbLinkTag</tagclass>
